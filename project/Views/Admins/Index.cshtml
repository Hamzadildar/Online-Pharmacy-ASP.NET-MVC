@model IEnumerable<project.Models.Admin>

@{
    ViewBag.Title = "Index";
}

<h2 class="mytext">Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.A_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.A_UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.A_Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.A_Cnic)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.A_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.A_UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.A_Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.A_Cnic)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
